      "CreateAirportDto": {
        "required": [
          "city",
          "iataId",
          "name",
          "servicingAreaId",
          "state",
          "streetAddress",
          "zipcode"
        ],
        "type": "object",
        "properties": {
          "iataId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "streetAddress": {
            "type": "string"
